{
  "manifest_version": 2,
  "name": "F2C",
  "description": "Converts in-page temperatures in degrees Fahrenheit to degrees Celsius",
  "version": "0.1.0",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.bundle.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "F2C"
  }
}